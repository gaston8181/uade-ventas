<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ar.com.uade.dao.model.Vendedor" table="VENDEDOR">
		<id name="legajo">
			<column name="LEGAJO" />
		</id>
		<property column="NOMBRE" name="nombre" />
		<property column="APELLIDO" name="apellido" />
		<property column="DNI" name="dni" />
		<property column="FECHANAC" name="fechaNac" />
		<property column="TELEFONO" name="telefono" />
		<property column="DOMICILIO" name="domicilio" />
		<property column="FECHAINGRESO" name="fechaIngreso" />
		<property column="FECHABAJA" name="fechaBaja" />

	</class>
</hibernate-mapping>


