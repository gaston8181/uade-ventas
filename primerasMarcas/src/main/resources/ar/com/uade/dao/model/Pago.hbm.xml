<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ar.com.uade.dao.model.Pago" table="PAGO">
        <id name="idPago">
        	<column name="IDPAGO"/>
        </id>    
        <property column="TOTALPAGO" 	name="totalPago"/>
     
    </class>
</hibernate-mapping>


